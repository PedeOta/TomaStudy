{
  "expo": {
    "name": "TomaStudy!",
    "slug": "tomastudy",
    "version": "1.0.0",
    "orientation": "portrait",

    "icon": "./assets/images/telasplash.png",

    "scheme": "tomastudy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/telasplash.png",
        "backgroundImage": "./assets/images/telasplash.png",
        "monochromeImage": "./assets/images/telasplash.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.TomaStudy"
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",

     [
  "expo-splash-screen",
  {
    "image": "./assets/images/telasplash.png",
    "resizeMode": "cover",
    "backgroundColor": "#ffffff"
  }
],

      "expo-web-browser"
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
